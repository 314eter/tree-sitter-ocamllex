{
  "name": "tree-sitter-ocamllex",
  "version": "0.20.1",
  "description": "OCamllex grammar for tree-sitter",
  "keywords": [
    "tree-sitter",
    "parser",
    "ocaml",
    "ocamllex"
  ],
  "homepage": "https://github.com/314eter/tree-sitter-ocamllex",
  "license": "MIT",
  "author": "Pieter Goetschalckx",
  "main": "bindings/node",
  "repository": "github:314eter/tree-sitter-ocamllex",
  "scripts": {
    "build": "tree-sitter generate",
    "test": "tree-sitter test"
  },
  "dependencies": {
    "nan": "^2.17.0"
  },
  "devDependencies": {
    "tree-sitter-cli": "^0.20.8"
  },
  "tree-sitter": [
    {
      "scope": "source.ocamllex",
      "file-types": [
        "mll"
      ],
      "injection-regex": "^(mll|ocamllex)$"
    }
  ]
}
